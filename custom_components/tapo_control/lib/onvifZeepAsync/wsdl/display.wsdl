			<xs:element name="Capabilities" type="tls:Capabilities"/>
			<!--===============================-->
			<xs:element name="GetLayout">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="VideoOutput" type="tt:ReferenceToken">
