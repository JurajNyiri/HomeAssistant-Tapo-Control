{
  "domain": "tapo_control",
  "name": "Tapo: Cameras Control",
  "documentation": "https://github.com/JurajNyiri/HomeAssistant-Tapo-Control",
  "issue_tracker": "https://github.com/JurajNyiri/HomeAssistant-Tapo-Control/issues",
  "codeowners": [
    "@JurajNyiri"
  ],
  "version": "6.1.9",
  "requirements": [
    "pytapo==3.3.46",
    "python-kasa[speedups]==0.10.2"
  ],
  "dependencies": [
    "ffmpeg",
    "onvif",
    "stream",
    "network"
  ],
  "config_flow": true,
  "homeassistant": "2024.1.0",
  "dhcp": [
    {
      "hostname": "c[0-9][0-9][0-9]*_*"
    },
    {
      "hostname": "d[0-9][0-9][0-9]*_*"
    },
    {
      "hostname": "tc[0-9][0-9]*_*"
    },
    {
      "hostname": "d[0-9][0-9][0-9]*c_*"
    },
    {
      "hostname": "h[0-9][0-9][0-9]*_*"
    }
  ],
  "loggers": [
    "custom_components.tapo_control"
  ]
}